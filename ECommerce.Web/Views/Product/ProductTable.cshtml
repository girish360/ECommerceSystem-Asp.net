@model List<Product>
@{
    ViewBag.Title = "ProductTable";
}

<h2>ProductTable</h2>

<table>
    <thead>
    <tr>
        <td>Product Name</td>
        <td>Category</td>
        <td>Price</td>
        <td>Actions</td>
    </tr>
    </thead>
    <tbody>
    @foreach (var product in Model)
    {
        <tr>
            <td>@product.Name</td>
            <td>-</td>
            <td>@product.price</td>
            <td>
                <button>Edit</button>
                <button>Delete</button>
            </td>
        </tr>
    }
    </tbody>
</table>